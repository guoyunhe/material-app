{
  "name": "material-app",
  "version": "2.1.0",
  "description": "All-in-one Material UI app solution",
  "keywords": [
    "react",
    "material-ui",
    "theme",
    "localization",
    "i18n",
    "authentication"
  ],
  "homepage": "https://github.com/guoyunhe/material-app#readme",
  "bugs": {
    "url": "https://github.com/guoyunhe/material-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guoyunhe/material-app.git"
  },
  "funding": "https://github.com/sponsors/guoyunhe",
  "license": "MIT",
  "author": {
    "name": "Guo Yunhe",
    "email": "i@guoyunhe.me",
    "url": "https://guoyunhe.me/"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rive build",
    "build:watch": "rive build --watch",
    "ci:eslint": "eslint -f json src -o ./.ci/eslint.json",
    "lint": "eslint .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "prepare": "husky",
    "start": "rive start",
    "test": "rive test",
    "test:ui": "rive test --ui",
    "test:watch": "rive test --watch"
  },
  "commitlint": {
    "extends": [
      "ali"
    ]
  },
  "lint-staged": {
    "*.{cjs,cts,js,jsx,mjs,mts,ts,tsx,vue}": "eslint --fix",
    "*.{cjs,css,cts,html,js,json,jsx,less,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}": "prettier --write"
  },
  "prettier": "prettier-config-ali",
  "stylelint": {},
  "dependencies": {
    "@guoyunhe/react-storage": "^2.0.0",
    "@guoyunhe/use-latest-callback": "^1.0.0",
    "material-compact": "^2.1.0",
    "react-fast-compare": "^3.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mdx-js/react": "^3.1.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "commitlint-config-ali": "^1.1.0",
    "eslint": "^9.30.0",
    "eslint-config-ali": "^16.3.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "prettier-config-ali": "^1.3.4",
    "react": "^18.3.1",
    "react-doc-ui": "^2.4.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "rive": "^3.1.2",
    "typescript": "^5.8.3",
    "xior": "^0.4.3"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.15.17 || ^6.0.0 || ^7.0.0",
    "@mui/material": "^5.15.17 || ^6.0.0 || ^7.0.0",
    "react": ">=16.8.0",
    "xior": "0.x"
  },
  "rive": {
    "template": "react",
    "doc": {
      "basename": "/material-app/"
    }
  }
}
